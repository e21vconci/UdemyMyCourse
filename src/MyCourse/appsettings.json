{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "Default": "Data Source=Data/MyCourse.db"
  },
  "Courses": {
    "PerPage": 10,
    "Order": {
      "By": "Rating",
      "Ascending": false,
      "Allow": ["Title", "Rating", "CurrentPrice"]
    }
  },
  "MemoryCache": {
    "TimeSpan": 60,
    "SizeLimit": 1000
  }
}
